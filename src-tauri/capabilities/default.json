{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "core:window:allow-show",
    "core:window:allow-start-dragging",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-load",
    "dialog:allow-open"
  ]
}
